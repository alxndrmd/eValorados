@model eValorados_Web.Models.Almacen

@{
    ViewBag.Title = "Edit";
    ViewBag.PageSection = " Editar Agencias";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edit</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Almacen</h4>
        <hr />
        @Html.ValidationSummary(true)
        @Html.ValidationMessage("CustomError")
        <div class="form-group">
            @Html.LabelFor(model => model.Agencia, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.Agencia.Descripcion, new { disabled = "disabled", @readonly = "readonly" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Valorado, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.Valorado.Descripcion, new { disabled = "disabled", @readonly = "readonly" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.CantidadMinima, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.CantidadMinima, new { @class = "control-label col-md-2" })
                @Html.ValidationMessageFor(model => model.CantidadMinima)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.CantidadMaxima, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.CantidadMaxima)
                @Html.ValidationMessageFor(model => model.CantidadMaxima)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.IsActivo, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.IsActivo)
                @Html.ValidationMessageFor(model => model.IsActivo)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
